<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/cmd">
    <Resource name="jdbc/cmd"
              auth="Container"
              type="javax.sql.DataSource"
              maxActive="100"
              maxIdle="30"
              maxWait="10000"
              username="cmd"
              password="cmd"
              driverClassName="oracle.jdbc.OracleDriver"
              url="jdbc:oracle:thin:@localhost:1521:xe"/>
</Context>